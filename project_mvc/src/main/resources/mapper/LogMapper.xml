<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caizhen.mvc.mapper.LogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.caizhen.pojo.entity.Log">
        <id column="id" property="id" />
        <result column="title" property="title" />
        <result column="content" property="content" />
        <result column="method" property="method" />
        <result column="request_method" property="requestMethod" />
        <result column="oper_name" property="operName" />
        <result column="request_url" property="requestUrl" />
        <result column="ip" property="ip" />
        <result column="ip_location" property="ipLocation" />
        <result column="request_param" property="requestParam" />
        <result column="response_result" property="responseResult" />
        <result column="status" property="status" />
        <result column="error_msg" property="errorMsg" />
        <result column="oper_time" property="operTime" />
        <result column="take_time" property="takeTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, title, content, method, request_method AS requestMethod, oper_name AS operName, request_url AS requestUrl, ip, ip_location AS ipLocation, request_param AS requestParam, response_result AS responseResult, status, error_msg AS errorMsg, oper_time AS operTime, take_time AS takeTime
    </sql>

</mapper>
